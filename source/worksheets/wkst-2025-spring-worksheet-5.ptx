              \fill[cyan] (0.6,0.2) circle(1.5);
              \end{scope}

              % Ensure V ∩ W is shaded underneath
              \begin{scope}
              \clip (-1.2,-0.8) circle(1.5);
              \clip (1.2,-0.8) circle(1.5);
              \fill[cyan] (0,-0.8) circle(1.5);
              \end{scope}

              % Define the circles
              \draw (0,1.2) circle(1.5);
              \draw (-1.2,-0.8) circle(1.5);
              \draw (1.2,-0.8) circle(1.5);

